{
  "name": "sakfjs",
  "version": "1.0.6",
  "description": "useful functions like a swiss army knife",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "scripts": {
    "start": "node lib/esm/index.js",
    "dev": "tsx src/index.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && tsc-alias -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json && tsc-alias -p ./configs/tsconfig.cjs.json",
    "clean": "rimraf ./lib",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "unitary": "vitest src/classes/SakfImage/SakfImage.test.ts"
  },
  "license": "ISC",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.28.4",
    "dotenv": "^16.0.3",
    "rimraf": "^4.1.2",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "jimp": "^0.22.4",
    "zod": "^3.20.3"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "files": [
    "lib/**/*"
  ]
}
